   "source": [
    "# Assignment - 8 (Leap Year)\n",
    "\n",
    "lpyr = int(input(\"Please enter a year:  \"))\n",
    "\n",
    "if lpyr % 4 == 0 and lpyr % 100 != 0 or lpyr % 400 == 0:\n",
    "\n",
    "    print (\"{0} is a leap year.\".format(lpyr))\n",
    "\n",
    "else:\n",
    "\n",
    "    print(\"{0} is a not leap year.\".format(lpyr))"
   ]
